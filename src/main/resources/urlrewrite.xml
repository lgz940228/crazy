<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN"
        "http://www.tuckey.org/res/dtds/urlrewrite4.0.dtd">

<!-- Configuration file for UrlRewriteFilter http://tuckey.org/urlrewrite/ -->
<urlrewrite>
    <!-- 首页 -->
	<rule>
		<from>^/$</from>
		<to>/api/index/index.do</to>
	</rule>
	<rule>
		<from>^/api/index/index.html$</from>
		<to>/api/index/index.do</to>
	</rule>
	<rule>
		<from>^/api/user/toLogin.html$</from>
		<to>/api/user/toLogin.do</to>
	</rule>
	<rule>
		<from>^/api/user/toRegister.html$</from>
		<to>/api/user/toRegister.do</to>
	</rule>
	<rule>
		<from>^/api/personal/personal.html$</from>
		<to>/api/personal/toPersonal.do</to>
	</rule>
	<rule>
		<from>^/api/shior/403.html$</from>
		<to>/api/shior/403.do</to>
	</rule>
	<rule>
		<from>^/api/shior/ttt.html$</from>
		<to>/api/shior/ttt.do</to>
	</rule>

</urlrewrite>
